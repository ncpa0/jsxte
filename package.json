{
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "esbuild": "^0.14.51",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-hook-tasks": "ncpa0cpl/git-hook-tasks",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "node-os-walk": "^1.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-jsdoc": "^0.4.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.7.4"
  },
  "name": "jsxte",
  "version": "2.2.1",
  "main": "./dist/legacy/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/types/jsx-runtime.d.ts",
      "import": "./dist/esm/jsx-runtime.mjs",
      "require": "./dist/cjs/jsx-runtime.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && yarn build:esm && yarn build:cjs && yarn build:types",
    "build:esm": "node ./scripts/build.mjs esmodule",
    "build:cjs": "node ./scripts/build.mjs commonjs",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly --outDir ./dist/types",
    "test:lint": "eslint .",
    "test:tsc": "tsc --noEmit",
    "test:jest": "jest --coverage"
  },
  "keywords": [
    "jsx",
    "template",
    "engine",
    "express",
    "node",
    "browser",
    "html"
  ],
  "repository": {
    "url": "https://github.com/ncpa0cpl/jsxte"
  },
  "description": "JSX-based html templating engine for browsers or Node environments.",
  "license": "MIT",
  "author": {
    "name": "Szymon Bretner (ncpa0cpl)",
    "email": "szymonb21@gmail.com"
  }
}
